{
  "name": "aot-asset-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "backend:migrate": "cd backend && npm run db:migrate",
    "backend:seed": "cd backend && npm run db:seed",
    "backend:test": "cd backend && npm test",
    "test:e2e": "tsx __tests__/e2e-test-script.ts",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "install:all": "npm install && cd backend && npm install && cd .."
  },
  "dependencies": {
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.554.0",
    "openai": "^6.9.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
